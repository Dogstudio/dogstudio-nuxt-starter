{
  "root": true,
  "env": {
    "node": true,
    "browser": true
  },
  "parser": "vue-eslint-parser",
  "parserOptions": {
    "parser": "espree",
    "sourceType": "module",
    "ecmaVersion": "latest"
  },
  "plugins": [
    "prettier"
  ],
  "extends": [
    "@nuxtjs",
    "plugin:nuxt/recommended",
    "plugin:prettier/recommended"
  ],
  "rules": {
    "no-undef": "off",
    "prettier/prettier": "error"
  }
}
